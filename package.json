{
  "name": "iconsax",
  "description": "iconsax for react and react native",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*",
      "website"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rendinjast/iconsax-react.git"
  },
  "version": "0.0.8",
"scripts": {
  "dev": "pnpm --filter website run dev",
  "start": "pnpm --filter website run start",
  "build": "lerna run build --ignore website",
  "gen": "node ./scripts/build.js",
  "bootstrap": "pnpm install -w",
  "website": "pnpm --filter website run",
  "website:dev": "pnpm --filter website run dev",
  "website:build": "pnpm --filter website run build",
  "website:start": "pnpm --filter website run start"
},
  "devDependencies": {
    "camelcase": "^6.2.0",
    "cheerio": "^1.0.0-rc.10",
    "lerna": "^4.0.0",
    "prettier-eslint": "^13.0.0"
  }
}
